<Page
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" Height="400" Width="500" Background="Black" HorizontalAlignment="Left">
  <Page.Resources>
    <!-- Base image -->
    <BitmapImage x:Key="ButtonImage" DecodePixelHeight="256" DecodePixelWidth="256" UriSource="Spider.png"/>
    
    <!-- Normal button colors -->
    <Color x:Key="NormalOuterEdgeColor">Gold</Color>
    
    <Color x:Key="NormalBandColorTop">White</Color>
    <Color x:Key="NormalBandColorBottom">Black</Color>
    
    <Color x:Key="NormalInnerEdgeColor">Gold</Color>
    
    <!-- Hover button colors -->
    <Color x:Key="HoverOuterEdgeColor">Gold</Color>
    
    <Color x:Key="HoverBandColorTop">White</Color>
    <Color x:Key="HoverBandColorBottom">Black</Color>
    
    <Color x:Key="HoverInnerEdgeColor">Gold</Color>
    
    <!-- Click button colors -->
    <!-- Disabled button colors -->
    
    <!-- Common elements -->
    <ImageBrush x:Key="ImageLayerBrush" Stretch="UniformToFill" ImageSource="{StaticResource ButtonImage}" />
    
    <SolidColorBrush x:Key="NormalOuterEdgeColorBrush" Color="{StaticResource NormalOuterEdgeColor}"/>
        
    <RadialGradientBrush x:Key="BackgroundGradient">    
                        <GradientStop Color="#FFFFFFFF" Offset="0.0" />
                        <GradientStop Color="#FFFCFCFC" Offset="0.1" />
                        <GradientStop Color="#FFF5F5F5" Offset="0.2" />
                        <GradientStop Color="#FFE8E8E8" Offset="0.3" />
                        <GradientStop Color="#FFD6D6D6" Offset="0.4" />
                        <GradientStop Color="#FFBFBFBF" Offset="0.5" />
                        <GradientStop Color="#FFA3A3A3" Offset="0.6" />
                        <GradientStop Color="#FF828282" Offset="0.7" />
                        <GradientStop Color="#FF5C5C5C" Offset="0.8" />
                        <GradientStop Color="#FF303030" Offset="0.9" />
                        <GradientStop Color="#FF000000" Offset="1.0" />
    </RadialGradientBrush>
    
    <LinearGradientBrush x:Key="NormalInnerBorderBand" StartPoint="531.006,-1.24" EndPoint="531.006,-76.4" MappingMode="Absolute" SpreadMethod="Pad">
      <GradientStop Offset="0" Color="{StaticResource NormalBandColorBottom}"/>
      <GradientStop Offset="1" Color="{StaticResource NormalBandColorTop}"/>
    </LinearGradientBrush>
    
    <LinearGradientBrush x:Key="HoverInnerBorderBand" StartPoint="531.006,-1.24" EndPoint="531.006,-76.4" MappingMode="Absolute" SpreadMethod="Pad">
      <GradientStop Offset="0" Color="{StaticResource HoverBandColorBottom}"/>
      <GradientStop Offset="1" Color="{StaticResource HoverBandColorTop}"/>
    </LinearGradientBrush>
    
    <Pen x:Key="NormalInnerBorder" DashCap="Round" EndLineCap="Round" LineJoin="Round" MiterLimit="4" StartLineCap="Round" Thickness="2.954"
         Brush="{StaticResource NormalInnerBorderBand}"/>

  <Style TargetType="Viewbox">
    <Setter Property="Width" Value="100"/>
    <Setter Property="Height" Value="100"/>
    
    <Setter Property="SnapsToDevicePixels" Value="True"/>
    
    <Setter Property="HorizontalAlignment" Value="Center"/>
    <Setter Property="VerticalAlignment" Value="Center"/>
    
    <Setter Property="Stretch" Value="UniformtoFill"/>
  </Style>
    
  </Page.Resources>
  
  <UniformGrid Columns="4" Width="500" HorizontalAlignment="Left">
  <Viewbox >
        <Grid Height="100" Width="100">
            <Ellipse
				Width="100"
				Height="100"
				HorizontalAlignment="Left"
				VerticalAlignment="Top"
				Stroke="{StaticResource NormalOuterEdgeColorBrush}" StrokeThickness="5" Fill="{StaticResource BackgroundGradient}">                
            </Ellipse>

            <Ellipse Width="88" Height="88" HorizontalAlignment="Left" Margin="6,6,0,0" VerticalAlignment="Top" Stroke="Transparent"
            Fill="{StaticResource ImageLayerBrush}"/>

            <Ellipse
				Width="100"
				Height="100"
				HorizontalAlignment="Left"
				VerticalAlignment="Top"
				Stroke="Transparent" StrokeThickness="1">
